 	 <animation>
  <type>material</type>
  <object-name>fuel-lb</object-name>
  <object-name>fuel-head</object-name>
  <object-name>fuel-lbext</object-name>
  <object-name>fuel-headext</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.0</red>
    <green>0.1</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.0</red>
    <green>0.8</green>
    <blue>0.0</blue>
  </emission> 
 </animation>
 	 <animation>
  <type>material</type>
  <object-name>bingo</object-name>

  <ambient>
    <red>0.1</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.1</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.8</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </emission> 
 </animation>



 <animation>
    <type>select</type>
    <object-name>fuel-lbext</object-name>
  <object-name>fuel-headext</object-name>
    <condition>
  <and>
      <greater-than>
       <property>consumables/fuel/tank[2]/level-lbs</property>
       <value>0</value>
      </greater-than>
      <greater-than>
       <property>consumables/fuel/tank[3]/level-lbs</property>
       <value>0</value>
      </greater-than>
</and>
    </condition>
  </animation>

 <animation>
    <type>select</type>
    <object-name>bingo</object-name>

    <condition>
  <and>
      <equals>
       <property>f22/isbingo</property>
       <value>1</value>
      </equals>
      <equals>
       <property>f22/blink</property>
       <value>1</value>
      </equals>
</and>
    </condition>
  </animation>


