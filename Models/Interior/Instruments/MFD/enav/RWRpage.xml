<?xml version="1.0"?>


<PropertyList>

 <model>
		<path>Aircraft/F-22/Models/Interior/Instruments/MFD/RWRpage.ac</path>
			<offsets>
				<x-m>0</x-m>
					<y-m>0</y-m>
				<z-m>0</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>

	
    <animation>
        <object-name>bkg</object-name>
        <type>material</type>
        <condition>
            <greater-than-equals>
                <property>systems/electrical/outputs/rwr</property>
                <value>20</value>
            </greater-than-equals>

        </condition>
        <emission>
            <red>  1</red>
            <green>1</green>
            <blue> 1</blue>
        </emission>
    </animation>

	
</PropertyList>
