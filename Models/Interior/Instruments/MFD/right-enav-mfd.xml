<?xml version="1.0"?>


<PropertyList>

<model>
		<path>Aircraft/F-22/Models/Interior/Instruments/MFD/blank.ac</path>
			<offsets>
				<x-m>0</x-m>
					<y-m>0</y-m>
				<z-m>0</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>

   
  
   <model>
    <path>Aircraft/F-22/Models/Interior/Instruments/MFD/enav/enavright.xml</path> <!--MFD Scripting-->
    <offsets>
      <x-m>-0.000</x-m>
      <y-m>0 </y-m>
      <z-m>-0.01</z-m>
    </offsets>
  </model>
  <animation>

  <object-name>pullup</object-name>


  <type>select</type>

            <condition>
                <not>
                
<property>gear/gear/position-norm</property>
</not>
                <and>
                    <greater-than>
                        <property>sim/model/radar/time-until-impact</property>
                        <value>0</value>
                    </greater-than>
                    <less-than>
                        <property>sim/model/radar/time-until-impact</property>
                        <value>8</value>
                    </less-than>
                </and>
                
            </condition>
 </animation>
</PropertyList>